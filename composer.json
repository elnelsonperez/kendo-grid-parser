{
    "name": "elnelsonperez/kendo-grid-parser",
    "version": "1.0",
    "description": "A package to apply any Kendo Grid filters to a query builder instance in Laravel.",
    "license": "MIT",
    "keywords": [
       "kendo",
        "laravel",
        "kendo grid",
        "laravel kendo",
        "parser"
    ],
    "authors": [
        {
            "name": "Nelson Perez",
            "email": "me@nelsonperez.net"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "ElNelsonPerez\\KendoGridParser\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ElNelsonPerez\\KendoGridParser\\KendoGridParserServiceProvider"
            ]
        }
